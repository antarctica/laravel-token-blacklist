{
    "name": "antarctica/laravel-token-blacklist",
    "description": "Enables API tokens to be marked as invalid until natural expiry within Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Fennell",
            "email": "felnne@bas.ac.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "indatus/dispatcher": "1.4.*@dev",
        "antarctica/laravel-base-exceptions": "dev-develop",
        "antarctica/laravel-base-repositories": "dev-develop",
        "antarctica/laravel-token-auth": "dev-develop"
    },
    "require-dev": {
        "illuminate/database": "~4.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "ssh://git@stash.ceh.ac.uk:7999/~felnne/laravel-base-exceptions.git"
        },
        {
            "type": "vcs",
            "url": "ssh://git@stash.ceh.ac.uk:7999/~felnne/laravel-base-repositories.git"
        },
        {
            "type": "vcs",
            "url": "ssh://git@stash.ceh.ac.uk:7999/~felnne/laravel-token-auth.git"
        }
    ],
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "Antarctica\\LaravelTokenBlacklist\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
